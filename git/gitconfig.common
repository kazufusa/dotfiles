[alias]
    hist = log --graph --pretty='format:%C(yellow)%h%Creset %C(magenta)%cd%Creset %Cgreen(%an)%Creset %s  %Cred%d%Creset' --date=iso
    co = checkout
    br = branch
    ci = commit
    st = status
[push]
    default = current
[github]
    user = kazufusa
[pager]
    diff = git diff -p --color | diff-highlight | less
    show = git show -p --color | diff-highlight | less
